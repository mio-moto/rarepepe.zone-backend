    "@elysiajs/swagger": "^0.7.2",
    "elysia": "^0.8.8",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "image-size": "^1.1.1",
    "js-nacl": "^1.4.0",
    "kysely": "^0.26.3",
    "kysely-bun-worker": "0.7.0",
    "nacl": "^0.1.3",
    "prettier": "^3.1.1",
    "tweetnacl": "^1.0.3",
    "winston": "^3.11.0",
